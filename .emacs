(set-face-attribute 'default nil :height 170) ;The value is in 1/10pt, so 100 will give you 10pt, etc.
(global-linum-mode 1) ;line numbers

(add-to-list 'default-frame-alist '(fullscreen . maximized)) ;fullscreen at startup

;set default font
(add-to-list 'default-frame-alist '(font . "Consolas" ))
(set-face-attribute 'default t :font "Consolas" )

;; open with single window
(setq inhibit-startup-screen t)
(add-hook 'emacs-startup-hook 'delete-other-windows)

